{
  "name": "chatbackend",
  "version": "1.0.0",
  "private": true,
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "dist/app.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test tap --node-arg=--require=ts-node/register \"test/**/*.test.ts\"",
    "start": "cross-env NODE_ENV=production npm run clean && npm run build:ts && node dist/app.js",
    "pm2": "pm2 start pm2.config.js && pm2-runtime pm2.config.js",
    "healthcheck": "node healthCheck.js",
    "build": "tsc",
    "build:ts": "tsc -p tsconfig.json",
    "cleanbuild": "run-s clean build:ts",
    "check-types": "tsc --noEmit --pretty",
    "docker": "docker build -t chatbackend . --no-cache",
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only app.ts",
    "format": "prettier-eslint --write \"{,!(node_modules)/**/}*.{js,jsx,ts,tsx}\"",
    "commit": "cz"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@fastify/autoload": "^5.7.1",
    "@fastify/caching": "^8.2.0",
    "@fastify/cors": "^8.2.0",
    "@fastify/formbody": "^7.4.0",
    "@fastify/helmet": "^10.1.0",
    "@fastify/redis": "^6.1.0",
    "@fastify/routes": "^5.1.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/swagger-ui": "^1.3.0",
    "@fastify/under-pressure": "^8.2.0",
    "@immobiliarelabs/fastify-sentry": "^5.0.2",
    "@mgcrea/fastify-graceful-exit": "^0.4.0",
    "@prisma/client": "^4.9.0",
    "abstract-cache": "^1.0.1",
    "abstract-cache-redis": "^2.0.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.12.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.5.0",
    "fastify-print-routes": "^2.0.7",
    "inclusion": "github:matherm-aboehm/inclusion#fix-1",
    "inquirer": "^8.2.5",
    "ioredis": "^5.3.0",
    "is-docker": "^3.0.0",
    "prisma-redis-middleware": "^4.6.0",
    "supertokens-node": "^12.1.6",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/tap": "^15.0.7",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-tap": "^1.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "prettier-eslint-cli": "^7.1.0",
    "prisma": "^4.9.0",
    "rimraf": "^3.0.2",
    "tap": "^16.3.4",
    "ts-node-dev": "^2.0.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
