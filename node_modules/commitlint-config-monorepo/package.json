  "main": "commitlint.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pskfyi/commitlint-config-monorepo.git"
  },
  "keywords": [
    "conventional-changelog",
    "commitlint",
    "commitlint-config"
  ],
  "author": "Patrick Keenan <patrick.keenan.fyi@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pskfyi/commitlint-config-monorepo/issues"
  },
  "homepage": "https://github.com/pskfyi/commitlint-config-monorepo#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "husky": "^6.0.0",
    "prettier": "^2.2.1"
  },
  "scripts": {
    "commitlint:pass": "commitlint --edit \"tests/pass.txt\" -V",
    "commitlint:fail": "commitlint --edit \"tests/fail.txt\" -V",
    "lint": "prettier -c .",
    "prepare": "husky install"
